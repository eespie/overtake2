; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={
}

[application]

config/name="Overtake 2"
run/main_scene="res://scenes/main/main.tscn"
boot_splash/image="res://splash.png"
boot_splash/fullsize=false
config/icon="res://icon.png"

[autoload]

events="*res://script/events.gd"
file="*res://script/file.gd"
scene="*res://script/scene.gd"
screen_mgr="*res://script/screen_mgr.gd"
race_mgr="*res://script/race_mgr.gd"
network="*res://script/network.gd"
message="*res://script/message.gd"
timer_mgr="*res://script/timer_mgr.gd"

[display]

window/size/width=1280
window/size/height=720
window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="2d"
window/stretch/aspect="keep_width"

[gdnative]

singletons=[  ]

[physics]

2d/default_linear_damp=3.0
2d/default_angular_damp=2

[rendering]

environment/default_environment="res://default_env.tres"
